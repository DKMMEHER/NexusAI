{
    "hosting": {
        "public": "frontend/dist",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "run": {
                    "serviceId": "nexusai",
                    "region": "asia-south1"
                }
            }
        ]
    }
}